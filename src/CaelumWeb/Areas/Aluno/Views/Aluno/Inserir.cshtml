@model CaelumWeb.Areas.Aluno.Models.Aluno

<form asp-controller="Aluno" asp-action="Inserir" method="post">
    <div class="form-group">
        <label asp-for="Nome"></label>
        <input asp-for="Nome" />
    </div>
    <div class="form-group">
        <label asp-for="Cpf"></label>
        <input asp-for="Cpf" />
    </div>
    <div class="form-group">
        <label asp-for="Endereco"></label>
        <input asp-for="Endereco" />
    </div>
    <div class="form-group">
        <label asp-for="Cidade"></label>
        <input asp-for="Cidade" />
    </div>
    <div class="form-group">
        <label asp-for="Estado"></label>
        <input asp-for="Estado" />
    </div>
    <input type="submit" value="Inserir" />
</form>

